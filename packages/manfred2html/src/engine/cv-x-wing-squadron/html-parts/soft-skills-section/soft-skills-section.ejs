<%_ if(softSkillsCollection && softSkillsCollection.length !== 0) { -%>
  <section class="main__section">
    <div class="main__section__header">
      <svg xmlns="http://www.w3.org/2000/svg" width="32" height="6" viewBox="0 0 32 6" fill="none">
        <path d="M5.51343e-07 3L32 3" stroke="#3BA0B7" stroke-width="6" />
      </svg>
      <h2><%= labels.SOFT_SKILLS_HEADING %></h2>
    </div>
    <ul class="main__section__skills__ul">
      <%_ for(const softSkill of softSkillsCollection) { -%>
        <li class="main__section__skills__li"><%= softSkill.skill.name %></li>
      <%_ } -%>
    </ul>
  </section>
<%_ } -%>
